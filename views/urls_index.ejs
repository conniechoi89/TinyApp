<!DOCTYPE html>
<html lang="en">
  <head>
    <title>TinyApp</title>
  </head>
  <body>
    <% include ../views/partials/_header %>
    <ul>
      <% for (let shortID in urlsDatabase) { %>
        <li> <%= shortID + ": " + urlsDatabase[shortID].longURL %>
          <form method="POST" action="/urls/<%= shortID %>/delete">
            <a href= "/urls/<%= shortID %>">Edit</a>
            <input type="submit" value="Delete">
          </form>
        </li>
      <% } %>
    </ul>
    <a href= "/urls/new">create short new url</a>
  </body>
</html>